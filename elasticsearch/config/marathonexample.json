{
  "id": "/elasticsearch",
  "cmd": null,
  "args": [
    "/home/elasticsearch/marathonrun.sh"
  ],
  "cpus": 0.25,
  "mem": 1536,
  "disk": 1536,
  "instances": 2,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ],
    [
      "hostname",
      "LIKE",
      "10.0.0.22|10.0.0.23"
    ]
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "flux159/elasticsearch",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 9200,
          "hostPort": 0,
          "servicePort": 30002,
          "protocol": "tcp",
          "labels": {}
        },
        {
          "containerPort": 9300,
          "hostPort": 0,
          "servicePort": 30003,
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "labels": {
    "HAPROXY_GROUP": "internal",
    "HAPROXY_0_VHOST": "elasticsearch.example.com"
  },
  "env": {
    "CLUSTER_NAME": "examplecluster",
    "APP_ID": "elasticsearch",
    "MARATHON_URL": "10.0.0.10:8080"
  },
  "portDefinitions": [
    {
      "port": 30002,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 30003,
      "protocol": "tcp",
      "labels": {}
    }
  ],
  "backoffSeconds": 30,
  "backoffFactor": 1.25,
  "maxLaunchDelaySeconds": 300,
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 0.5
  }
}